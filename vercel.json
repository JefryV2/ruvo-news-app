{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "expo export --platform web",
        "outputDirectory": "web-build"
      }
    }
  ],
  "routes": [
    {
      "src": "/_expo/(.*)",
      "dest": "/_expo/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.+\\.js)",
      "dest": "/$1"
    },
    {
      "src": "/(.+\\.json)",
      "dest": "/$1"
    },
    {
      "src": "/(.+\\.png)",
      "dest": "/$1"
    },
    {
      "src": "/(.+\\.jpg)",
      "dest": "/$1"
    },
    {
      "src": "/(.+\\.jpeg)",
      "dest": "/$1"
    },
    {
      "src": "/(.+\\.gif)",
      "dest": "/$1"
    },
    {
      "src": "/(.+\\.svg)",
      "dest": "/$1"
    },
    {
      "src": "/(.+\\.ico)",
      "dest": "/$1"
    },
    {
      "src": "/(.+\\.css)",
      "dest": "/$1"
    },
    {
      "src": "/(.+\\.ttf)",
      "dest": "/$1"
    },
    {
      "src": "/(.+\\.woff)",
      "dest": "/$1"
    },
    {
      "src": "/(.+\\.woff2)",
      "dest": "/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}